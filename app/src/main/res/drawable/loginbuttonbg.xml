<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">



    <item android:state_pressed="false"
        android:state_enabled="true">
        <shape>
            <gradient
                android:startColor="#33be31"
                android:endColor="#23942e"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#dcdcdc" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>


    <item android:state_pressed="true"
            android:state_enabled="true">
        <shape>
            <gradient
                android:startColor="#8833be31"
                android:endColor="#8823942e"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#dcdcdc" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>



    <item android:state_enabled="false" >
        <shape>
            <gradient
                android:startColor="#c8c8c8"
                android:endColor="#a8ced0d2"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#dcdcdc" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>



</selector>